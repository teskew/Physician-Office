<h1>Sign Up</h1>


<%= form_for(@user, url: signup_path) do |f|%>
    <%= f.label :username %>
    <%= f.text_field :name %>
    <br><br>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <br><br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br><br>
    <%= f.submit "Sign Up" %>
<% end %>