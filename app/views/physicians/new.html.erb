   
   <h1>Create A New Physician</h1>
 
<%= form_for(@physician) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <h3>Add Appointements:</h3>
  <%= f.fields_for :appointements do |s| %>
        <%= s.label :date %>
        <%= s.datetime_field :date%><br/>
      
    <hr>
  <% end %>
  <%= f.submit %>
  <% end %>


